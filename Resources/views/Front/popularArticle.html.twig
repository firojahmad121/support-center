{% set popArticles = ""%}
{% if popArticles %}
    <div class="uv-paper-aside">
        <aside>
            <h3>{{'Popular Articles'|trans}}</h3>
            <ul>
                {% for popArticle in popArticles %}
                    <li><a href="{{path('webkul_support_center_front_solution_article_slug', {'slug': popArticle.slug})}}"> {% if popArticle.stared %}<span class="uv-icon-star uv-pre"></span>{% endif %} {{ popArticle.translatedName is defined and popArticle.translatedName ? popArticle.translatedName :popArticle.name }}</a></li>
                    {#<li><a href="{{path('webkul_support_center_front_solution_category_article', {'article': popArticle.id})}}"> {% if popArticle.stared %}<span class="uv-icon-star uv-pre"></span>{% endif %} {{popArticle.name}}</a></li>#}
                {% endfor %}
            </ul>
        </aside>
    </div>
{% endif %}